{{ 'cutouts.css' | asset_url | stylesheet_tag }}



<div class="container marg-bot-xl marg-top-xl">
  <h2 class="text-center marg-bot-xl">{{ section.settings.title }}</h2>

  <div class="flex-box justify-around flex-wrap cutout-container no-mobile">
    {%- for block in section.blocks -%}
    {% case block.type %}
    {%- when 'cutout_image' -%}
    <div class="img-div marg-top marg-bot">
      <div class="original-img">
        
        {% if block.settings.original_image != blank %}
        <img srcset="{%- if block.settings.original_image.width >= 165 -%}{{ block.settings.original_image | img_url: '165x' }} 165w,{%- endif -%}
        {%- if block.settings.original_image.width >= 360 -%}{{ block.settings.original_image | img_url: '360x' }} 360w,{%- endif -%}
        {%- if block.settings.original_image.width >= 533 -%}{{ block.settings.original_image | img_url: '533x' }} 533w,{%- endif -%}
        {%- if block.settings.original_image.width >= 720 -%}{{ block.settings.original_image | img_url: '720x' }} 720w,{%- endif -%}
        {%- if block.settings.original_image.width >= 940 -%}{{ block.settings.original_image | img_url: '940x' }} 940w,{%- endif -%}
        {%- if block.settings.original_image.width >= 1066 -%}{{ block.settings.original_image | img_url: '1066x' }} 1066w{%- endif -%}"
        src="{{ block.settings.original_image | img_url: '533x' }}"
        sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
        alt="{{ block.settings.original_image.alt | escape }}"
        loading="lazy"
        class="motion-reduce"
        width="{{ block.settings.original_image.width }}"
        height="{{ block.settings.original_image.height }}"
        >
        {% else %}
        <div class="placeholder">{{ 'image' | placeholder_svg_tag }}</div>
        {% endif %}
        
      </div>
      <div class="cutout-img no-disp">
        
        {% if block.settings.hover_image != blank %}

          <img srcset="{%- if block.settings.hover_image.width >= 165 -%}{{ block.settings.hover_image | img_url: '165x' }} 165w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 360 -%}{{ block.settings.hover_image | img_url: '360x' }} 360w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 533 -%}{{ block.settings.hover_image | img_url: '533x' }} 533w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 720 -%}{{ block.settings.hover_image | img_url: '720x' }} 720w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 940 -%}{{ block.settings.hover_image | img_url: '940x' }} 940w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 1066 -%}{{ block.settings.hover_image | img_url: '1066x' }} 1066w{%- endif -%}"
          src="{{ block.settings.hover_image | img_url: '533x' }}"
          sizes="(min-width: 1100px) 535px, (min-width: 750px) calc((100vw - 130px) / 2), calc((100vw - 50px) / 2)"
          alt="{{ block.settings.hover_image.alt | escape }}"
          loading="lazy"
          class="motion-reduce"
          width="{{ block.settings.hover_image.width }}"
          height="{{ block.settings.hover_image.height }}"
          >
        {% else %}
        <div class="placeholder">{{ 'image' | placeholder_svg_tag  }}</div>
        {% endif %}
        
      </div>
    </div>
    {%- endcase -%}
    {%- endfor -%}


  </div>

  <div class="flex-box justify-around flex-wrap cutout-container no-desktop">
    {%- for block in section.blocks -%}
    {% case block.type %}
    {%- when 'cutout_image' -%}
    <div class="img-container marg-top marg-bot">
      <div class="original-img">
         {% if block.settings.original_image != blank %}
         <img srcset="{%- if block.settings.original_image.width >= 165 -%}{{ block.settings.original_image | img_url: '165x' }} 165w,{%- endif -%}
        {%- if block.settings.original_image.width >= 360 -%}{{ block.settings.original_image | img_url: '360x' }} 360w,{%- endif -%}
        {%- if block.settings.original_image.width >= 533 -%}{{ block.settings.original_image | img_url: '533x' }} 533w,{%- endif -%}
        {%- if block.settings.original_image.width >= 720 -%}{{ block.settings.original_image | img_url: '720x' }} 720w,{%- endif -%}
        {%- if block.settings.original_image.width >= 940 -%}{{ block.settings.original_image | img_url: '940x' }} 940w,{%- endif -%}
        {%- if block.settings.original_image.width >= 1066 -%}{{ block.settings.original_image | img_url: '1066x' }} 1066w{%- endif -%}"
        src="{{ block.settings.original_image | img_url: '533x' }}"
        alt="{{ block.settings.original_image.alt | escape }}"
        loading="lazy"
        class="motion-reduce"
        width="{{ block.settings.original_image.width }}"
        height="{{ block.settings.original_image.height }}"
        >
        {% else %}
        <div class="placeholder">{{ 'image' | placeholder_svg_tag : 'placeholder-svg' }}</div>
        {% endif %}
      </div>
      <div class="cutout-img no-disp">
        {% if block.settings.hover_image != blank %}
        <img srcset="{%- if block.settings.hover_image.width >= 165 -%}{{ block.settings.hover_image | img_url: '165x' }} 165w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 360 -%}{{ block.settings.hover_image | img_url: '360x' }} 360w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 533 -%}{{ block.settings.hover_image | img_url: '533x' }} 533w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 720 -%}{{ block.settings.hover_image | img_url: '720x' }} 720w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 940 -%}{{ block.settings.hover_image | img_url: '940x' }} 940w,{%- endif -%}
          {%- if block.settings.hover_image.width >= 1066 -%}{{ block.settings.hover_image | img_url: '1066x' }} 1066w{%- endif -%}"
          src="{{ block.settings.hover_image | img_url: '533x' }}"
          alt="{{ block.settings.hover_image.alt | escape }}"
          loading="lazy"
          class="motion-reduce"
          width="{{ block.settings.hover_image.width }}"
          height="{{ block.settings.hover_image.height }}"
          >
        {% else %}
      <div class="placeholder">{{ 'image' | placeholder_svg_tag: 'placeholder-svg' }}</div>
        {% endif %}
      </div>
    </div>
    {%- endcase -%}
    {%- endfor -%}


  </div>

</div>


{% schema %}
{
    "name": "t:sections.cards-with-hover-effect.name",
    "settings": [
        {
            "id": "title",
            "type": "html",
            "label": "t:sections.cards-with-hover-effect.settings.heading.label"
        }
    ],
    "blocks": [
        {
            "type": "cutout_image",
            "name": "t:sections.cards-with-hover-effect.blocks.cards.name",
            "settings": [
                {
                    "id": "original_image",
                    "type": "image_picker",
                    "label": "t:sections.cards-with-hover-effect.blocks.cards.settings.normal_image.label",
                    "info": "t:sections.cards-with-hover-effect.blocks.cards.settings.normal_image.info"
                },
                {
                    "id": "hover_image",
                    "type": "image_picker",
                    "label": "t:sections.cards-with-hover-effect.blocks.cards.settings.hover_image.label",
                    "info": "t:sections.cards-with-hover-effect.blocks.cards.settings.hover_image.info"
                }
            ]
        }
    ],
    "presets": [
        {
            "name": "t:sections.cards-with-hover-effect.presets.name",
            "category": "ADVANCED LAYOUT"
        }
    ]
}
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

