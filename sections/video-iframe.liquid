
<section class="video-iframe">
   <div class="container">
  <h2 class="iframe-heading">{{ section.settings.header_text }}</h2>
    {%- if section.settings.iframe_url != blank -%}    
    {{ section.settings.iframe_url }}
    {%- else -%}
    {{ 'lifestyle-1' | placeholder_svg_tag: 'placeholder-svg'  }}
    {% endif %}
     </div>
</section>


{% schema %}
{
    "name": "t:sections.video-iframe.name",
    "settings": [
        {
            "id": "header_text",
            "type": "text",
            "label": "t:sections.video-iframe.settings.header_text.label",
            "default": "Video heading"
        },
        {
            "id": "iframe_url",
            "type": "html",
            "label": "t:sections.video-iframe.settings.iframe_url.label",
            "info": "t:sections.video-iframe.settings.iframe_url.info"
        }
    ],
    "blocks": [],
    "presets": [
        {
            "name": "t:sections.video-iframe.presets.name",
            "category": "ADVANCED LAYOUT"
        }
    ]
}
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
