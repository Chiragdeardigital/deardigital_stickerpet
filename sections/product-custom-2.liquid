 function AfterBodyResize() {
  var curr = $(".step-selected").text();
  //     $(".steps").addClass("no-disp");

  switch (curr) {
    case "1":
      $(".steps").addClass("no-disp");
      $(".first-step").removeClass("no-disp");
      $("#Steps-button").text("Step 2");
      if ($(".type-selected").length <= 0) {
        $("#Steps-button").attr("disabled", true);
      }
      $("#AddToCart--product-custom-template-2").addClass("no-disp");
      break;

    case "2":
      $(".steps").addClass("no-disp");
      $(".second-step").removeClass("no-disp");
      $("#Steps-button").text("Step 3");
      if ($(".selected-frame").length <= 0) {
        $("#Steps-button").attr("disabled", true);
      }
      $("#AddToCart--product-custom-template-2").addClass("no-disp");
      break;

    case "3":
      $(".steps").addClass("no-disp");
      $(".third-step").removeClass("no-disp");
      $("#Steps-button").text("Step 4");
      if ($(".selected-frame-size").length <= 0) {
        $("#Steps-button").attr("disabled", true);
      }
      $("#AddToCart--product-custom-template-2").addClass("no-disp");
      break;

    case "4":
      $(".steps").addClass("no-disp");
      $(".fourth-step").removeClass("no-disp");
      $("#Steps-button").text("Step 5");
      $("#Steps-button").attr("disabled", true);
      $("#AddToCart--product-custom-template-2").addClass("no-disp");
      break;

    case "5":
      $(".steps").addClass("no-disp");
      $(".fifth-step").removeClass("no-disp");
      $("#Steps-button").addClass("no-disp");
      $("#AddToCart--product-custom-template-2").removeClass("no-disp");
      if ($(".hanging-product-selected").length <= 0) {
        $("#AddToCart--product-custom-template-2").attr("disabled", true);
      }
      break;

    default:
      $(".steps").addClass("no-disp");
      $(".first-step").removeClass("no-disp");
      $("#Steps-button").text("Step 2");
      if ($(".type-selected").length <= 0) {
        $("#Steps-button").attr("disabled", true);
      }
      break;
  }
}
  
  $(window).resize(function(){
    if(window.outerWidth < 768) {
    	$(".steps").removeClass("no-disp")
        $("#AddToCart--product-custom-template-2").removeClass("no-disp");
    }
    else {
        AfterBodyResize();
    }
  });
  
  if(window.outerWidth < 768) {
    	$(".steps").removeClass("no-disp");
        $("#AddToCart--product-custom-template-2").removeClass("no-disp");
  }
  else {
        AfterBodyResize();
  }
  
