<div class="blue-background">
<!-- /templates/product.liquid -->
{% assign current_variant = product.selected_or_first_available_variant %}
{% assign featured_image = current_variant.featured_image | default: product.featured_image %}
{%- assign first_3d_model = product.media | where: "media_type", "model" | first -%}

<script>
  variants_json = {}
  variant_colors = []
  variant_framesizes = []
  variant_frametype = []

  {% for variant in product.variants %}
	{% if variant.available %}
      var title = "{{ variant.title }}"
      title = title.replace(/ /g, "");
      variants_json[title] = "{{ variant.id }}"
      var color = title.split("/")[0];
  	  var framesize = title.split("/")[1];
  	  var frametype = title.split("/")[2];
  	  if(!variant_colors.includes(color)) {
        variant_colors.push(color)
      }
  	  if(!variant_framesizes.includes(framesize)) {
        variant_framesizes.push(framesize)
      }
  	  if(!variant_frametype.includes(frametype)) {
  	    variant_frametype.push(frametype)
      }
    {% endif %}
  {% endfor %}

</script>

<div class="wrapper padd-bot-xl min-height-element" itemscope itemtype="http://schema.org/Product" id="ProductSection--{{ section.id }}"
