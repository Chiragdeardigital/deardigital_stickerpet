    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    text-align: center;
    transition: .4s ease-out;
    transform: translateY(120%);
    display: block
}

.topbar-usp-item.is-visible {
    transform: translateY(0)
}

.topbar-usp-item.is-finished {
    transform: translateY(-120%)
}

.topbar-usp-item.is-hidden {
    display: none
}

