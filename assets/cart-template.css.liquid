.ProgressBar {
  display: block;
  position: relative;
  background-color: #fff;
  width: 100%;
  height: 10px;
  margin: 20px 0;
}
.UnprogressBar {
  position: absolute;
  top: 55px;
  height: 10px;
  width: 85%;
  background-color: #a5dcde;
  z-index: 100;
  -webkit-transition: width 1s ease-in 1s;
  -moz-transition: width 1s ease-in 1s;
  -o-transition: width 1s ease-in 1s;
  transition: width 1s ease-in 1s;
}

.UnprogressBar.unclicked {
  width: 0%;
}
