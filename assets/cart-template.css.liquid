.cart-body {
  display: block;
  margin: 0 auto;
  padding: 0;
  height: calc(100vh - 520px);
  overflow-y: auto;
}

.ajaxcart__product {
  position: relative;
  max-height: 500px;
}

.ajaxcart__product.is-removed {
  max-height: 0;
  overflow: hidden;
  visibility: hidden;
  backface-visibility: hidden;
  transition: all 450ms cubic-bezier(0.57,.06,.05,.95);
}

.ajaxcart__row {
  padding-bottom: 17px;
  margin-bottom: 17px;
  border-bottom: 1px solid #062322;
}

.grid {
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}

.grid__item.one-quarter {
  width: calc(25% - 15px);
}

.ajaxcart__product-image {
  padding-top: 3px;
  display: block;
  overflow: hidden;
}

.ajaxcart__product-image img {
  display: block;
  margin: 0 auto;
  max-width: 100%;
}

.ajaxcart__product-name--wrapper {
  margin-bottom: 8px;
}

.ajaxcart__product-name {
  font-size: 15px;
  line-height: 1.3;
  display: block;
  color: #062322;
}

.grid__item.three-quarters {
  width: calc(75% - 15px);
}

.grid__item.one-half {
  width: calc(50% - 10px);
  /*   text-align: end; */
}

.ajaxcart__qty, .js-qty {
  position: relative;
  max-width: 75px;
  min-width: 75px;
  overflow: visible;
}

.ajaxcart__qty-adjust {
  padding: 0 5px;
  line-height: 1;
  color: #062322;
  z-index: 1;
  cursor: pointer;
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 14px;
  top: 0;
  bottom: 0;
  border: 0 none;
  background: 0 0;
  text-align: center;
  overflow: hidden;
  user-select: none;
  transition: all .2s ease-out;
}

.ajaxcart__qty--minus {
  left: 0;
  border-right: 1px solid #062322;
}

.ajaxcart__qty--plus {
  right: 0;
  border-left: 1px solid #062322;
}

.ajaxcart__qty-adjust .icon {
  display: inline-block;
  font-size: 8px;
  vertical-align: middle;
}

.drawer .ajaxcart__qty-num[type=text] {
  font-family: Lato, sans-serif;
  letter-spacing: .1em;
  text-transform: uppercase;
  font-style: normal;
  font-weight: 400;
  display: block;
  background: 0 0;
  text-align: center;
  width: 100%;
  padding: 2px 20px;
  border: 1px solid #062322;
}
.ajaxcart__qty-adjust:hover {
  background-color: #062322;
  color: #fff;
}

.ajaxcart__price, .cart__price, .cart__subtotal {
  font-family: Lato, sans-serif;
  font-style: normal;
  letter-spacing: .1em;
  text-transform: uppercase;
  font-weight: 400;
  font-size: .88235em;
  display: block;
}

.cart-foot {
  display: block;
  position: sticky;
  top: 100%;
  padding-top: 8px;
  margin-top: 8px;
  border-top: 1px solid #062322;
}
#add-gift {
  cursor: pointer;
}

.one-third {
  width: calc(33.333% - 20px);
}

.two-thirds {
  width: calc(66.666% - 20px);
  text-align: right;
}

.ajaxcart__subtotal {
  font-family: Lato,sans-serif;
  font-weight: 700;
  font-style: normal;
  letter-spacing: .5px;
  text-transform: none;
  font-size: 12px;
  margin-bottom: 5px;
}
.text-right .ajaxcart__ship, .text-right .ajaxcart__subtotal {
  font-weight: 400;
}
.text-right {
  text-align: right;
}

.ajaxcart__ship {
  font-size: 12px;
  font-family: Lato,sans-serif;
  font-weight: 700;
  font-style: normal;
  letter-spacing: .5px;
  text-transform: none;
  margin-bottom: 5px;
}
.ajaxcart__product-meta, .ajaxcart__product-name {
  display: block;
}

.ajaxcart__product-name+.ajaxcart__product-meta {
  padding-top: 3px;
}
.display-non {
  display: none!important;
}

.cart__checkout{
  width: 100%;
  border-radius: 5px;
  margin-bottom: 10px;
}
.continue-shopping {
  background-color: #fff;
  color: #f9ae37;
  border: 2px solid #f9ae37;
}

.ajaxcart__note {
  font-weight: 400;
}


.ajaxcart__inner--has-fixed-footer::-webkit-scrollbar, .cart-body::-webkit-scrollbar, .drawer__inner::-webkit-scrollbar {
  display: none;
}

