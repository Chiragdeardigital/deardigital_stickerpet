/* -----------------------------------------------------
ANNOUNCEMENT STYLING
----------------------------------------------------- */

.site-header {
  /* background-color: rgb(var(--color-base-background-1)); */
  padding: 15px 0;
}


.header-container {
  z-index: 2;
  position: relative;
}

.announcement-bar {
  display: block;
  text-align: center;
  position: relative;
}

.topbar-usp-list {
  width: 100%;
  height: 46px;
  position: relative;
  overflow: hidden;
}

.topbar-usp-item {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  text-align: center;
  transition: 0.4s ease-out;
  transform: translateY(120%);
  display: block;
}

.topbar-usp-item.is-visible {
  transform: translateY(0);
}

.topbar-usp-item.is-finished {
  transform: translateY(-120%);
}

.topbar-usp-item.is-hidden {
  display: none;
}

.announcement-bar__message {
  display: block;
  font-weight: 700;
  padding: 10px 30px;
  letter-spacing: 1px;
}

