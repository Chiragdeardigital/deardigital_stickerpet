<div class="drawer__inner drawer-left__inner">
  <ul class="mobile-nav">
   

    {%- for block in section.blocks -%}
    {%- case block.type -%}
    {%- when 'mobile_menu' -%}
    {%- for link in linklists[block.settings.mobile_menu].links -%}
    <li class="mobile-nav__item">
      {%- if link.links != blank -%}
      <div class="mobile-nav__has-sublist">
        <div class="mobile-nav__toggle">
          <div class="mobile-shop">
            {%- endif -%}
            <a href="{{ link.url }}" class="{% if link.links != blank %}mobile-shp{% endif %} mobile-nav__link"
               id="Label-{{ forloop.index }}"> {{ link.title }}</a>
            {%- if link.links != blank -%}
            <button type="button" class="mobile-shop-btn mobile-nav__toggle-btn icon-fallback-text"
                    aria-controls="Linklist-0" aria-expanded="false" data-list-id="Linklist-{{ forloop.index }}">
              <div class="plus-icon-wrap">
                <div class="toggle open"></div>
              </div>
            </button>
          </div>
        </div>
      </div>
      <ul class="mobile-nav__sublist content-list" id="Linklist-{{ forloop.index }}" role="navigation">
        {%- for child_link in link.links -%} 
        {%- assign product = child_link.object -%}

        <li class="accordian mobile-nav__item">
          <a href="{{product.url}}" class="mobile-nav__link">Custom {{ product.title }}
            {%- if product.images.size > 0 -%}
            {%- if product.images.size > 1 -%}
            {%- assign product_image = product.images[1] -%}
            {%- else -%}
            {%- assign product_image = product.featured_image -%}
            {%- endif -%}
            <img srcset="{%- if product_image.width >= 60 -%}{{ product_image | img_url: '60x' }} 60w,{%- endif -%}"
                 src="{{ product_image | img_url: '60x' }}" 
                 alt="{{ product.title }}" 
                 width="auto"
                 height="auto"
                 style="width: 60px; margin-top: -10px;"
                 loading="lazy"
                 />
            {%- else -%}
            {{ 'product-1' | placeholder_svg_tag }}
            {%- endif -%}
          </a>

          {% if child_link.links != blank %}
          <details-disclosure id="grandlink_mobile">
            <details open>
              <summary>
                <button type="button" class="mobile-shop-btn-2 mobile-nav__toggle-btn icon-fallback-text"
                    aria-controls="Linklist-0" aria-expanded="false" data-list-idd="GrandLinklist-{{ forloop.index }}">
                  <div class="plus-icon-wrap">
                    <div class="toggle toggle-grand open"></div>
                  </div>
                </button>
              </summary>
              <ul class="mobile-nav__sublist content-list" id="GrandLinklist-{{ forloop.index }}" role="navigation">
                {%- for grand_child_link in child_link.links -%} 
                {%- assign product = grand_child_link.object -%}
                    <li class="mobile_granchild mobile-nav__item" >
                      <a href="{{product.url}}" class="mobile-nav__link">Custom {{ product.title }}</span> 
                        {%- if product.images.size > 0 -%}
                        {%- if product.images.size > 1 -%}
                        {%- assign product_image = product.images[1] -%}
                        {%- else -%}
                        {%- assign product_image = product.featured_image -%}
                        {%- endif -%}
                        <img srcset="{%- if product_image.width >= 60 -%}{{ product_image | img_url: '60x' }} 60w,{%- endif -%}"
                            src="{{ product_image | img_url: '60x' }}" 
                            alt="{{ product.title }}" 
                            width="auto"
                            height="auto"
                            style="width: 60px; margin-top: -10px;"
                            loading="lazy"
                            />

                        {%- else -%}
                        {{ 'product-1' | placeholder_svg_tag }}
                        {%- endif -%}
                      </a>
                    </li>
                    {%- endfor -%}
              </ul>
          </li>
        </details>
      </details-disclosure>
      {% endif %}
      {%- endfor -%}
      </ul>
      {%- endif -%}
    </li>
    {%- endfor -%}
    {%- endcase -%}
    {%- endfor -%}

